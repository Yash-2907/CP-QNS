//arithmatic operations
#include<stdio.h>
int main()
{
  int x,y;
  printf("pls enter two numbers :- ");
  scanf("%d%d",&x,&y);
  printf("Sum of number =%d\n",x+y);
  printf("Difference of number =%d\n",x-y);
  printf("Product of number =%d\n",x*y);
  printf("Quotient of number =%d\n",x/y);
  printf("Remainder of number =%d\n",x%y);
  return 0;
}
